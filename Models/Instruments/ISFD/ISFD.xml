<?xml version="1.0"?>
<PropertyList>
    <path>ISFD.ac</path>
	<!-- ISFD -->
	
	<animation>
		<type>knob</type>
        <object-name>ISFD-baro</object-name>
        <center>
            <x-m>0.011315</x-m>
            <y-m>0.025963</y-m>
            <z-m>-0.025992</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
		<property>/controls/ISFD/animation-3D/btn-baro-knob</property>
        <factor>5</factor>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/ISFD/animation-3D/btn-baro-knob</property>
                <offset>1</offset>
                <min>0</min>
                <max>500</max>                
                <wrap>0</wrap>
            </binding>
        </action>
	</animation>
	
	<animation>
