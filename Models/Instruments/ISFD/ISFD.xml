				<command>property-assign</command>
				<property>controls/ISFD/animation-3D/btn-rst</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/ISFD/animation-3D/btn-rst</property>
					<value>0</value>
				</binding>
				<binding>
					<command>nasal</command>
					<script>isfd.rstBtn();</script>
				</binding>
			</mod-up>
		</action>
	</animation>
	
