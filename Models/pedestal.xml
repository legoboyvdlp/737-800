        </center>
        <factor>30</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/fires/fire[1]/extinguish-handle-pos</property>
                <factor>1</factor> 
				<min>-1</min>
				<max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
	
	<animation>
		<object-name>Circle.169</object-name>
		<type>material</type>
		<emission>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
			<factor-prop>/controls/fires/fire[0]/on-fire</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<object-name>Circle.170</object-name>
		<type>material</type>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>/controls/fires/fire[2]/on-fire</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<object-name>Circle.171</object-name>
		<type>material</type>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>/controls/fires/fire[1]/on-fire</factor-prop>
