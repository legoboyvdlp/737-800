<?xml version="1.0"?>
<PropertyList>
    <path>pedestal.ac</path>
    <animation>
        <type>select</type>
        <object-name>EICAS2</object-name>
        <condition>
            <property>sim/current-view/internal</property>
        </condition>
    </animation>

    <model>
        <name>EICAS2</name>
        <path>Models/Instruments/EICAS_Lower/eicas.xml</path>
        <offsets>
            <x-m>0.426</x-m>
            <y-m>-0.021</y-m>
            <z-m>0.738</z-m>
            <pitch-deg>299.5</pitch-deg>
        </offsets>
    </model>
	
    <model>
        <name>CDU1</name>
        <path>Aircraft/Instruments-3d/cdu2/boeing.xml</path>
        <offsets>
            <x-m>0.460</x-m>
            <y-m>-0.203</y-m>
            <z-m>0.725</z-m>
            <pitch-deg>299.5</pitch-deg>
        </offsets>
        <overlay>
            <texture>boeing_grey.png</texture>
        </overlay>

    </model>

    <model>
        <name>CDU2</name>
        <path>Aircraft/Instruments-3d/cdu2/boeing.xml</path>
        <offsets>
            <x-m>0.460</x-m>
            <y-m>0.162</y-m>
            <z-m>0.725</z-m>
            <pitch-deg>299.5</pitch-deg>
        </offsets>
        <overlay>
            <texture>boeing_grey.png</texture>
        </overlay>
    </model>
    
    <animation>
        <type>translate</type>
        <object-name>Circle.169</object-name>
        <property>/controls/fires/fire[0]/extinguish-handle-armed</property>
        <factor>0.02</factor>
        <axis>
           <x>0</x>
           <y>0</y>
           <z>1</z>
        </axis>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>Circle.169</object-name>
        <condition>
            <and>
                <not>
                    <property>/controls/fires/fire[0]/extinguish-handle-armed</property>
                </not>
                <property>/controls/fires/fire[0]/on-fire</property>
            </and>
        </condition>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/controls/fires/fire[0]/extinguish-handle-armed</property>
                <value>1</value>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>Circle.170</object-name>
        <property>/controls/fires/fire[2]/extinguish-handle-armed</property>
        <factor>0.02</factor>
        <axis>
           <x>0</x>
           <y>0</y>
           <z>1</z>
        </axis>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>Circle.170</object-name>
        <condition>
            <and>
                <not>
                    <property>/controls/fires/fire[2]/extinguish-handle-armed</property>
                </not>
                <property>/controls/fires/fire[2]/on-fire</property>
            </and>
        </condition>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/controls/fires/fire[2]/extinguish-handle-armed</property>
                <value>1</value>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>Circle.171</object-name>
        <property>/controls/fires/fire[1]/extinguish-handle-armed</property>
        <factor>0.02</factor>
        <axis>
           <x>0</x>
           <y>0</y>
           <z>1</z>
        </axis>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>Circle.171</object-name>
        <condition>
            <and>
                <not>
                    <property>/controls/fires/fire[1]/extinguish-handle-armed</property>
                </not>
                <property>/controls/fires/fire[1]/on-fire</property>
            </and>
        </condition>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/controls/fires/fire[1]/extinguish-handle-armed</property>
                <value>1</value>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>Circle.169</object-name>
        <property>/controls/fires/fire[0]/extinguish-handle-pos</property>
        <condition>
                <property>/controls/fires/fire[0]/extinguish-handle-armed</property>
        </condition>
        <factor>30</factor>
        <center>
            <x-m>0.872059</x-m>
            <y-m>-0.097029</y-m>
            <z-m>0.60127</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/fires/fire[0]/extinguish-handle-pos</property>
                <factor>1</factor> 
				<min>-1</min>
				<max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>Circle.170</object-name>
        <property>/controls/fires/fire[2]/extinguish-handle-pos</property>
        <condition>
                <property>/controls/fires/fire[2]/extinguish-handle-armed</property>
        </condition>
