        <center>
            <x-m>0.872454</x-m>
            <y-m>-0.020167</y-m>
            <z-m>0.60127</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/fires/fire[2]/extinguish-handle-pos</property>
                <factor>1</factor>  
				<min>-1</min>
				<max>1</max>				
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>Circle.171</object-name>
        <property>/controls/fires/fire[1]/extinguish-handle-pos</property>
        <condition>
                <property>/controls/fires/fire[1]/extinguish-handle-armed</property>
        </condition>
